{
    "name": "时序书签",
    "version": "0.0.3",
    "manifest_version": 3,
    "description": "An extension for sorting bookmarks by the time they were added.",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "icons": {
        "16": "icons/bookmark16x16.png",
        "32": "icons/bookmark32x32.png",
        "48": "icons/bookmark48x48.png",
        "128": "icons/bookmark128x128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icons/bookmark16x16.png",
            "32": "icons/bookmark32x32.png",
            "48": "icons/bookmark48x48.png",
            "128": "icons/bookmark128x128.png"
        }
    }
}